<?xml version="1.0" encoding="utf-8"?>
<bootui xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<boot_cmds>
		<cmd>
			<id>CmdRoot</id>
			<category>Group</category>
			<children>
				<CmdID>CmdOverLockDialogs</CmdID>
				<CmdID>CmdCommonDialogs</CmdID>
				<CmdID>CmdExtractAndUpdate</CmdID>
				<CmdID>CmdDownloadAssets</CmdID>
			</children>
		</cmd>
		<cmd>
			<id>CmdDownloadAssets</id>
			<category>Window</category>
			<window_id>WinDownloadAssets</window_id>
		</cmd>
		<cmd>
			<id>CmdExtractAndUpdate</id>
			<category>Window</category>
			<window_id>WinExtractAndUpdate</window_id>
		</cmd>
		<cmd>
			<id>CmdCommonDialogs</id>
			<category>Stack</category>
			<children>
				<CmdID>CmdDialogMessage</CmdID>
				<CmdID>CmdDialogYesNoMessage</CmdID>
			</children>
		</cmd>
		<cmd>
		    <id>CmdDialogMessage</id>
		    <category>Window</category>
		    <window_id>WinDialogMessage</window_id>
		    <lock_back>true</lock_back>
		    <darken_back>true</darken_back>
		    <click_back_to_close>false</click_back_to_close>
		</cmd>
		<cmd>
		    <id>CmdDialogYesNoMessage</id>
		    <category>Window</category>
		    <window_id>WinDialogYesNoMessage</window_id>
		    <lock_back>true</lock_back>
		    <darken_back>true</darken_back>
		    <click_back_to_close>false</click_back_to_close>
		</cmd>
		<cmd>
			<id>CmdOverLockDialogs</id>
			<category>Stack</category>
			<children>
				<CmdID>CmdOverLockDialogMessage</CmdID>
				<CmdID>CmdOverLockDialogYesNoMessage</CmdID>
			</children>
		</cmd>
		<cmd>
			<id>CmdOverLockDialogMessage</id>
			<category>Window</category>
			<window_id>WinDialogMessage</window_id>
			<lock_back>true</lock_back>
			<darken_back>true</darken_back>
			<click_back_to_close>false</click_back_to_close>
		</cmd>
		<cmd>
			<id>CmdOverLockDialogYesNoMessage</id>
			<category>Window</category>
			<window_id>WinDialogYesNoMessage</window_id>
			<lock_back>true</lock_back>
			<darken_back>true</darken_back>
			<click_back_to_close>false</click_back_to_close>
			<allow_multi_instances>true</allow_multi_instances>
		</cmd>
	</boot_cmds>
	<boot_windows>
		<window>
			<id>WinDialogMessage</id>
			<resource>ui_message_box</resource>
			<occasion>OCCASION_COMMON</occasion>
		</window>
		<window>
			<id>WinDialogYesNoMessage</id>
			<resource>ui_message_box_opaque</resource>
			<occasion>OCCASION_COMMON</occasion>
		</window>
		<window>
		    <id>WinExtractAndUpdate</id>
		    <resource>ui_extract</resource>
		    <occasion>OCCASION_BOOT</occasion>
		</window>
		<window>
		    <id>WinDownloadAssets</id>
		    <resource>ui_download_assets</resource>
		    <occasion>OCCASION_BOOT</occasion>
		</window>
	</boot_windows>
</bootui>